{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "no-console": 1,
    "no-case-declarations": 0,
    "react/display-name": 0,
    "react/prop-types": 0,
  },
  "globals": {
    "Promise": false
  }
}
